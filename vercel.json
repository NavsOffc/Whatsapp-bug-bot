{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/send-bug.js": {
      "maxDuration": 30
    },
    "api/status.js": {
      "maxDuration": 10
    }
  }
}
